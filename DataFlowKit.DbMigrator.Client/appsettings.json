{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "server=.;Database=DemoDbMigrator;Trusted_Connection=True;MultipleActiveResultSets=true;Encrypt=True;TrustServerCertificate=True;Connection Timeout=30;"
  },
  "DapperMigrator": {
    "EnvironmentName": "QA",
    "ProviderName": "sqlserver",
    "MigrationProject": "DataFlowKit.DbMigrator.Client",
    "MigrationDirectory": "Migrations",
    "EntityProject": "DataFlowKit.DbMigrator.Client",
    "EntityDirectory": "SpEntity",
    "EntityModelNamingConvention": "DBO"
  },
  "AllowedHosts": "*"
}
